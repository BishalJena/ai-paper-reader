{
  "name": "mlpaper-interact-frontend",
  "version": "1.0.0",
  "description": "Frontend for ML Paper Explorer: live code & interactive plots",
  "scripts": {
    "lint": "eslint static/**/*.js",
    "test": "jest"
  },
  "dependencies": {
    "thebe": "^0.9.3",
    "plotly.js": "^2.24.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "eslint": "^8.35.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "jest": {
    "testMatch": [
      "**/tests/**/*.js",
      "**/tests/**/*.jsx"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/@testing-library/jest-dom/extend-expect.js"
    ]
  }
}

